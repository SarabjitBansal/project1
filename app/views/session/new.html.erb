<div class ="formfood">
  <h1>Log in</h1>
  <h2><%= flash[:error] %></h2>
  <%= form_tag login_path do  %>
    <div class = "formlog">
        <%= label_tag  :email %>
        <%= email_field_tag :email %>

        <%= label_tag  :password %>
        <%= password_field_tag :password %>
        <label></label>


        <%= submit_tag "Log in", :class => "btnlogin" %>
    </div>
</div>







<% end %>
